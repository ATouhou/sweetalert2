{
  "name": "sweetalert2",
  "version": "7.6.2",
  "repository": "sweetalert2/sweetalert2",
  "homepage": "https://sweetalert2.github.io/",
  "description": "A beautiful, responsive, customizable and accessible (WAI-ARIA) replacement for JavaScript's popup boxes, supported fork of sweetalert",
  "main": "dist/sweetalert2.all.js",
  "jsnext:main": "src/sweetalert2.js",
  "types": "sweetalert2.d.ts",
  "devDependencies": {
    "babel-core": "latest",
    "babel-plugin-array-includes": "latest",
    "babel-plugin-external-helpers": "latest",
    "babel-plugin-transform-object-assign": "latest",
    "babel-preset-es2015": "latest",
    "bundlesize": "^0.15.3",
    "execa": "~0.9.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-autoprefixer": "^4.1.0",
    "gulp-clean-css": "^3.9.0",
    "gulp-concat": "^2.6.1",
    "gulp-css2js": "^1.0.2",
    "gulp-if": "^2.0.2",
    "gulp-load-plugins": "^1.5.0",
    "gulp-rename": "latest",
    "gulp-rollup": "^2.16.1",
    "gulp-sass": "latest",
    "gulp-sass-lint": "latest",
    "gulp-standard": "^8.0.0",
    "gulp-tslint": "^8.1.2",
    "gulp-uglify": "^3.0.0",
    "merge2": "^1.2.1",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-json": "^2.3.0",
    "standard": "^8.0.0",
    "testem": "latest",
    "tslint": "^5.8.0",
    "typescript": "^2.5.3"
  },
  "standard": {
    "ignore": [
      "dist/"
    ]
  },
  "files": [
    "dist",
    "src",
    "sweetalert2.d.ts"
  ],
  "author": "Limon Monte <limon.monte@gmail.com> (https://limonte.github.io)",
  "contributors": [
    "Morgan Touverey-Quilling <mtouverey@alembic-dev.com> (https://github.com/toverux)",
    "Johan Fagerberg (https://github.com/birjolaxew)",
    "Sam Turrell <sam@samturrell.co.uk> (https://github.com/samturrell)",
    "Joseph Schultz (https://github.com/acupajoe)",
    "Matthew Francis Brunetti <zenflow87@gmail.com> (https://github.com/zenflow)",
    "Patrick H. Lauke (https://github.com/patrickhlauke)"
  ],
  "keywords": [
    "sweetalert",
    "sweetalert2",
    "alert",
    "prompt",
    "confirm"
  ],
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "npm run build && npm run check",
    "build": "gulp build",
    "check": "npm run check:bundlesize && npm run check:lint && npm run check:test && npm run check:dts",
    "check:bundlesize": "bundlesize -f dist/sweetalert2.all.min.js -s 15kB",
    "check:lint": "gulp lint",
    "check:test": "testem ci",
    "check:dts": "tsc sweetalert2.d.ts",
    "fix:lint": "standard --fix",
    "dev": "gulp dev --skip-minification --skip-standalone"
  },
  "bugs": "https://github.com/sweetalert2/sweetalert2/issues",
  "license": "MIT"
}
